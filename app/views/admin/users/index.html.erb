<h1>Users</h1>
<%= link_to "Back to home page!", root_path, class: "btn btn-md btn-success" %>

<%= paginate @users %>

<% @users.each do |user| %><br>
  <h3 class="admin-view-user"><%= link_to user.email, admin_user_path(user) %></h3>
  <p><%= user.firstname %>, <%= user.lastname %></p>
<% end %>


